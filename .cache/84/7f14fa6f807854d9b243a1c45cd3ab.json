inal":{"line":52,"column":15}}],"sources":{"components/buttoncontroller.js":"import React, {useContext} from \"react\";\nimport Button from \"./button\";\nimport {TimeContext} from \"./TimeContext\";\n\nconst ButtonController = (props) => {\n    const [timer, setTimer] = useContext(TimeContext);\n\n    const resetTime = () => {\n        console.log(timer);\n        setTimer({\n            ...timer,\n            work: 1500,\n            break: 300,\n            mode: \"work\",\n            time: {currentTime: 1500, startingTime: 1500},\n            active: false,\n            name: \"Pomodoro Timer\",\n            progress: 0,\n        });\n        props.myRef.current.pause();\n        props.myRef.current.currentTime = 0;\n    };\n\n    const setActive = () => {\n        setTimer({\n            ...timer,\n            active: !timer.active,\n        });\n    };\n\n    return (\n        <div className=\"ButtonController\">\n            <Button\n                buttonId=\"reset\"\n                type=\"reset\"\n                actionClick={() => resetTime()}>\n                <i className=\"fas fa-undo-alt\"></i>\n            </Button>\n            <Button\n                buttonId=\"start_stop\"\n                type=\"play\"\n                actionClick={() => setActive()}>\n                <i\n                    className={`fas ${\n                        timer.active ? \"fa-pause\" : \"fa-play\"\n                    }`}></i>\n            </Button>\n        </div>\n    );\n};\n\nexport default ButtonController;\n"},"lineCount":null}},"error":null,"hash":"e970de69b85d28646d934b6ffd551cb7","cacheData":{"env":{}}}